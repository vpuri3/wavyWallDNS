C     Budgets for standard averaging (apart from those in AVG)

      real dtime
      common /avgtimes/ dtime

      COMMON /javip/ pm1(lx1,ly1,lz1,lelv)
     $ , pm1avg(lx1,ly1,lz1,lelv), pmrms(lx1,ly1,lz1,lelv)


      common /variances/ uvari(lx1,ly1,lz1,lelt),
     $     vvari(lx1,ly1,lz1,lelt), wvari(lx1,ly1,lz1,lelt),
     $     uvcov(lx1,ly1,lz1,lelt), vwcov(lx1,ly1,lz1,lelt),
     $     wucov(lx1,ly1,lz1,lelt),
     $     tvari(lx1,ly1,lz1,lelt),pvari(lx1,ly1,lz1,lelt)


